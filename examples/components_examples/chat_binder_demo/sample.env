# Bot token from BotFather
BOT_TOKEN=your_bot_token_here

# MongoDB connection string
MONGO_URI=mongodb://localhost:27017

# Enable chat binder component
BOTSPOT_CHAT_BINDER_ENABLED=True

# MongoDB collection for chat binder
BOTSPOT_CHAT_BINDER_MONGO_COLLECTION=chat_binder

# Make chat binder commands visible in command menu
BOTSPOT_CHAT_BINDER_COMMANDS_VISIBLE=True